{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "npm install && npm run build"
    }
  }
  